{
  "name": "@orcapt/cli",
  "version": "0.4.1",
  "description": "CLI tool for managing Orca projects - Quick setup and deployment",
  "main": "src/index.js",
  "bin": {
    "orcapt": "bin/orca.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
