    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "simple-git": "^3.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orcapt/orca-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Orcapt/orca-cli/issues"
  },
  "homepage": "https://github.com/Orcapt/orca-cli#readme",
